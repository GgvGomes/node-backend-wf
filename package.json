{
  "name": "teste-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon --exec 'node -r ts-node/register/transpile-only -r tsconfig-paths/register' src/index.ts",
    "test": "jest",
